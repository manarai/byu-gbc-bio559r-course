    "### Exercise 2.1: Parameter Estimation for Enzyme Kinetics"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generate synthetic experimental data\n",
    "def generate_enzyme_data(Vmax_true=100, Km_true=5, noise_level=0.1, n_points=20):\n",
    "    \"\"\"\n",
    "    Generate synthetic Michaelis-Menten data with noise\n",
    "    \"\"\"\n",
    "    # Substrate concentrations (experimental design)\n",
    "    S_exp = np.logspace(-1, 2, n_points)  # 0.1 to 100\n",
    "    \n",
    "    # True reaction rates\n",
    "    v_true = (Vmax_true * S_exp) / (Km_true + S_exp)\n",
    "    \n",
    "    # Add experimental noise\n",
    "    noise = np.random.normal(0, noise_level * v_true)\n",
    "    v_observed = v_true + noise\n",
    "    \n",
    "    # Ensure non-negative rates\n",
    "    v_observed = np.maximum(v_observed, 0.1)\n",
    "    \n",
    "    return S_exp, v_observed, v_true\n",
    "\n",
    "# Generate experimental data\n",
    "S_data, v_data, v_true_data = generate_enzyme_data()\n",
    "\n",
    "print(f\"Generated {len(S_data)} experimental data points\")\n",
    "print(f\"Substrate range: {S_data.min():.2f} to {S_data.max():.2f}\")\n",
    "print(f\"Rate range: {v_data.min():.2f} to {v_data.max():.2f}\")\n",
    "\n",
    "# Define objective function for parameter estimation\n",
    "def michaelis_menten_objective(params, S_data, v_data):\n",
    "    \"\"\"\n",
    "    Objective function for Michaelis-Menten parameter estimation\n",
    "    \"\"\"\n",
    "    Vmax, Km = params\n",
    "    \n",
    "    # Ensure positive parameters\n",
    "    if Vmax <= 0 or Km <= 0:\n",
    "        return np.inf\n",
    "    \n",
    "    # Predicted rates\n",
    "    v_pred = (Vmax * S_data) / (Km + S_data)\n",
    "    \n",
    "    # Sum of squared residuals\n",
    "    residuals = v_data - v_pred\n",
    "    return np.sum(residuals**2)\n",
    "\n",
    "# Perform optimization\n",
    "initial_guess = [80, 3]  # Starting values for Vmax, Km\n",
    "bounds = [(1, 200), (0.1, 50)]  # Parameter bounds\n",
    "\n",
    "result = optimize.minimize(\n",
    "    michaelis_menten_objective,\n",
    "    initial_guess,\n",
    "    args=(S_data, v_data),\n",
    "    bounds=bounds,\n",
    "    method='L-BFGS-B'\n",
    ")\n",
    "\n",
    "Vmax_est, Km_est = result.x\n",
    "\n",
    "print(f\"\\nOptimization Results:\")\n",
    "print(f\"True parameters: Vmax = 100, Km = 5\")\n",
    "print(f\"Estimated parameters: Vmax = {Vmax_est:.2f}, Km = {Km_est:.2f}\")\n",
    "print(f\"Optimization success: {result.success}\")\n",
    "print(f\"Final objective value: {result.fun:.2f}\")\n",
    "\n",
    "# Calculate R-squared\n",
    "v_pred_final = (Vmax_est * S_data) / (Km_est + S_data)\n",
    "ss_res = np.sum((v_data - v_pred_final)**2)\n",
    "ss_tot = np.sum((v_data - np.mean(v_data))**2)\n",
    "r_squared = 1 - (ss_res / ss_tot)\n",
    "print(f\"R-squared: {r_squared:.3f}\")"
   ]
