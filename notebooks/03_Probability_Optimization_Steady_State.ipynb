   "source": [
    "# Simulate molecular noise in gene expression\n",
    "def simulate_molecular_noise(n_cells=1000, mean_expression=50, noise_level=0.3):\n",
    "    \"\"\"\n",
    "    Simulate gene expression with different noise models\n",
    "    \n",
    "    Parameters:\n",
    "    n_cells: number of cells to simulate\n",
    "    mean_expression: average expression level\n",
    "    noise_level: relative noise strength\n",
    "    \"\"\"\n",
    "    \n",
    "    # 1. Poisson noise (intrinsic noise)\n",
    "    poisson_expression = np.random.poisson(mean_expression, n_cells)\n",
    "    \n",
    "    # 2. Log-normal noise (extrinsic noise)\n",
    "    sigma = noise_level\n",
    "    mu = np.log(mean_expression) - 0.5 * sigma**2  # Adjust for log-normal mean\n",
    "    lognormal_expression = np.random.lognormal(mu, sigma, n_cells)\n",
    "    \n",
    "    # 3. Negative binomial (overdispersed)\n",
    "    # Parameterize using mean and dispersion\n",
    "    dispersion = 2.0  # Higher = more overdispersed\n",
    "    p = mean_expression / (mean_expression + dispersion)\n",
    "    r = dispersion\n",
    "    negbinom_expression = np.random.negative_binomial(r, 1-p, n_cells)\n",
    "    \n",
    "    return {\n",
    "        'poisson': poisson_expression,\n",
    "        'lognormal': lognormal_expression,\n",
    "        'negative_binomial': negbinom_expression\n",
    "    }\n",
    "\n",
    "# Generate noise data\n",
    "noise_data = simulate_molecular_noise()\n",
    "\n",
    "# Calculate statistics\n",
    "noise_stats = []\n",
    "for model, data in noise_data.items():\n",
    "    noise_stats.append({\n",
    "        'model': model,\n",
    "        'mean': np.mean(data),\n",
    "        'variance': np.var(data),\n",
    "        'cv': np.std(data) / np.mean(data),  # Coefficient of variation\n",
    "        'fano_factor': np.var(data) / np.mean(data)  # Variance-to-mean ratio\n",
    "    })\n",
    "\n",
    "stats_df = pd.DataFrame(noise_stats)\n",
    "print(\"Molecular noise statistics:\")\n",
    "print(stats_df.round(3))\n",
    "\n",
    "# Prepare data for visualization\n",
    "noise_viz_data = []\n",
    "for model, data in noise_data.items():\n",
    "    for value in data:\n",
    "        noise_viz_data.append({\n",
    "            'model': model,\n",
    "            'expression': value\n",
    "        })\n",
    "\n",
    "noise_df = pd.DataFrame(noise_viz_data)\n",
    "print(f\"\\nNoise data prepared: {len(noise_df)} observations\")"
   ]
