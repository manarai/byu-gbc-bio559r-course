    "def repressilator_model(y, t, alpha, n, beta):\n",
    "    \"\"\"\n",
    "    Repressilator: three-gene oscillator\n",
    "    \n",
    "    Gene 1 represses Gene 2\n",
    "    Gene 2 represses Gene 3  \n",
    "    Gene 3 represses Gene 1\n",
    "    \"\"\"\n",
    "    x1, x2, x3 = y\n",
    "    \n",
    "    dx1_dt = alpha / (1 + x3**n) - beta * x1\n",
    "    dx2_dt = alpha / (1 + x1**n) - beta * x2\n",
    "    dx3_dt = alpha / (1 + x2**n) - beta * x3\n",
    "    \n",
    "    return [dx1_dt, dx2_dt, dx3_dt]\n",
    "\n",
    "# Parameters for oscillatory behavior\n",
    "alpha = 20    # High production rate\n",
    "n = 4         # High cooperativity\n",
    "beta = 1      # Degradation rate\n",
    "\n",
    "# Initial condition\n",
    "y0 = [1, 0.5, 2]\n",
    "\n",
    "# Time points\n",
    "t = np.linspace(0, 20, 1000)\n",
    "\n",
    "# Solve ODE\n",
    "solution = odeint(repressilator_model, y0, t, args=(alpha, n, beta))\n",
    "\n",
    "# Create DataFrame for visualization\n",
    "repressilator_df = pd.DataFrame({\n",
    "    'time': np.tile(t, 3),\n",
    "    'expression': np.concatenate([solution[:, 0], solution[:, 1], solution[:, 2]]),\n",
    "    'gene': ['Gene 1'] * len(t) + ['Gene 2'] * len(t) + ['Gene 3'] * len(t)\n",
    "})\n",
    "\n",
    "# Analyze oscillation properties\n",
    "def analyze_oscillations(time_series, time_points):\n",
    "    \"\"\"\n",
    "    Analyze oscillation properties\n",
    "    \"\"\"\n",
    "    # Find peaks\n",
    "    from scipy.signal import find_peaks\n",
    "    \n",
    "    peaks, _ = find_peaks(time_series, height=np.mean(time_series))\n",
    "    \n",
    "    if len(peaks) > 1:\n",
    "        # Calculate period\n",
    "        periods = np.diff(time_points[peaks])\n",
    "        mean_period = np.mean(periods)\n",
    "        \n",
    "        # Calculate amplitude\n",
    "        amplitude = (np.max(time_series) - np.min(time_series)) / 2\n",
    "        \n",
    "        return mean_period, amplitude, len(peaks)\n",
    "    else:\n",
    "        return None, None, 0\n",
    "\n",
    "# Analyze each gene\n",
    "oscillation_analysis = []\n",
    "for i, gene in enumerate(['Gene 1', 'Gene 2', 'Gene 3']):\n",
    "    period, amplitude, n_peaks = analyze_oscillations(solution[:, i], t)\n",
    "    oscillation_analysis.append({\n",
    "        'gene': gene,\n",
    "        'period': period,\n",
    "        'amplitude': amplitude,\n",
    "        'n_peaks': n_peaks\n",
    "    })\n",
    "\n",
    "osc_df = pd.DataFrame(oscillation_analysis)\n",
    "print(\"Repressilator Oscillation Analysis:\")\n",
    "print(osc_df.round(3))\n",
    "\n",
    "print(f\"\\nSystem exhibits oscillations: {osc_df['n_peaks'].mean() > 1}\")\n",
    "if osc_df['period'].notna().any():\n",
    "    print(f\"Average period: {osc_df['period'].mean():.2f} time units\")"
   ]
