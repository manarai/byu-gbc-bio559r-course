   "source": [
    "def gillespie_gene_expression(k_on, k_off, k_m, gamma_m, k_p, gamma_p, t_max=100, n_simulations=50):\n",
    "    \"\"\"\n",
    "    Gillespie algorithm for stochastic gene expression\n",
    "    \n",
    "    Model: Gene switching + transcription + translation\n",
    "    G_off <-> G_on -> mRNA -> Protein\n",
    "    \n",
    "    Parameters:\n",
    "    k_on, k_off: gene switching rates\n",
    "    k_m: transcription rate\n",
    "    gamma_m: mRNA degradation rate\n",
    "    k_p: translation rate\n",
    "    gamma_p: protein degradation rate\n",
    "    \"\"\"\n",
    "    \n",
    "    results = []\n",
    "    \n",
    "    for sim in range(n_simulations):\n",
    "        # Initial state: [gene_state, mRNA, protein]\n",
    "        # gene_state: 0 = off, 1 = on\n",
    "        state = [0, 0, 0]\n",
    "        t = 0\n",
    "        \n",
    "        trajectory = []\n",
    "        \n",
    "        while t < t_max:\n",
    "            gene_state, mRNA, protein = state\n",
    "            \n",
    "            # Calculate reaction propensities\n",
    "            propensities = [\n",
    "                k_on * (1 - gene_state),      # Gene activation\n",
    "                k_off * gene_state,           # Gene deactivation\n",
    "                k_m * gene_state,             # Transcription\n",
    "                gamma_m * mRNA,               # mRNA degradation\n",
    "                k_p * mRNA,                   # Translation\n",
    "                gamma_p * protein             # Protein degradation\n",
    "            ]\n",
    "            \n",
    "            total_propensity = sum(propensities)\n",
    "            \n",
    "            if total_propensity == 0:\n",
    "                break\n",
    "            \n",
    "            # Sample time to next reaction\n",
    "            tau = np.random.exponential(1 / total_propensity)\n",
    "            t += tau\n",
    "            \n",
    "            # Sample which reaction occurs\n",
    "            r = np.random.random() * total_propensity\n",
    "            cumsum = 0\n",
    "            \n",
    "            for i, prop in enumerate(propensities):\n",
    "                cumsum += prop\n",
    "                if r <= cumsum:\n",
    "                    # Execute reaction i\n",
    "                    if i == 0:  # Gene activation\n",
    "                        state[0] = 1\n",
    "                    elif i == 1:  # Gene deactivation\n",
    "                        state[0] = 0\n",
    "                    elif i == 2:  # Transcription\n",
    "                        state[1] += 1\n",
    "                    elif i == 3:  # mRNA degradation\n",
    "                        state[1] = max(0, state[1] - 1)\n",
    "                    elif i == 4:  # Translation\n",
    "                        state[2] += 1\n",
    "                    elif i == 5:  # Protein degradation\n",
    "                        state[2] = max(0, state[2] - 1)\n",
    "                    break\n",
    "            \n",
    "            # Record state\n",
    "            if len(trajectory) == 0 or t - trajectory[-1]['time'] > 0.5:  # Sample every 0.5 time units\n",
    "                trajectory.append({\n",
    "                    'time': t,\n",
    "                    'gene_state': state[0],\n",
    "                    'mRNA': state[1],\n",
    "                    'protein': state[2],\n",
    "                    'simulation': sim\n",
    "                })\n",
    "        \n",
    "        results.extend(trajectory)\n",
    "    \n",
    "    return pd.DataFrame(results)\n",
    "\n",
    "# Parameters for bursty gene expression\n",
    "params = {\n",
    "    'k_on': 0.1,      # Gene activation rate\n",
    "    'k_off': 1.0,     # Gene deactivation rate (fast)\n",
    "    'k_m': 10.0,      # Transcription rate (high when on)\n",
    "    'gamma_m': 1.0,   # mRNA degradation\n",
    "    'k_p': 5.0,       # Translation rate\n",
    "    'gamma_p': 0.1    # Protein degradation (slow)\n",
    "}\n",
    "\n",
    "print(\"Running Gillespie simulation...\")\n",
    "gillespie_results = gillespie_gene_expression(**params, n_simulations=20)\n",
    "print(f\"Simulation completed: {len(gillespie_results)} time points\")\n",
    "print(f\"Final protein levels: mean = {gillespie_results.groupby('simulation')['protein'].last().mean():.1f}\")"
   ]
  },
