   "source": [
    "### Exercise 3.1: Bistable Switch Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Analyze bistable switch from previous notebook\n",
    "def bistable_switch_analysis(alpha=5, beta=1, n=2):\n",
    "    \"\"\"\n",
    "    Analyze steady states of a bistable switch\n",
    "    \n",
    "    System: du/dt = alpha/(1+v^n) - beta*u\n",
    "            dv/dt = alpha/(1+u^n) - beta*v\n",
    "    \"\"\"\n",
    "    \n",
    "    def find_steady_states():\n",
    "        \"\"\"Find steady states numerically\"\"\"\n",
    "        def steady_state_equations(vars):\n",
    "            u, v = vars\n",
    "            du_dt = alpha / (1 + v**n) - beta * u\n",
    "            dv_dt = alpha / (1 + u**n) - beta * v\n",
    "            return [du_dt, dv_dt]\n",
    "        \n",
    "        # Try different initial guesses\n",
    "        initial_guesses = [\n",
    "            [0.1, 0.1],   # Low-low state\n",
    "            [4.0, 0.5],   # High-low state\n",
    "            [0.5, 4.0],   # Low-high state\n",
    "            [2.5, 2.5]    # Symmetric state\n",
    "        ]\n",
    "        \n",
    "        steady_states = []\n",
    "        for guess in initial_guesses:\n",
    "            try:\n",
    "                sol = optimize.fsolve(steady_state_equations, guess)\n",
    "                # Check if it's actually a steady state\n",
    "                residual = steady_state_equations(sol)\n",
    "                if np.max(np.abs(residual)) < 1e-6:\n",
    "                    # Check if we already found this state\n",
    "                    is_new = True\n",
    "                    for existing in steady_states:\n",
    "                        if np.allclose(sol, existing, atol=1e-3):\n",
    "                            is_new = False\n",
    "                            break\n",
    "                    if is_new:\n",
    "                        steady_states.append(sol)\n",
    "            except:\n",
    "                pass\n",
    "        \n",
    "        return steady_states\n",
    "    \n",
    "    def analyze_stability(steady_state):\n",
    "        \"\"\"Analyze stability using Jacobian matrix\"\"\"\n",
    "        u_ss, v_ss = steady_state\n",
    "        \n",
    "        # Jacobian matrix\n",
    "        J11 = -beta  # âˆ‚(du/dt)/âˆ‚u\n",
    "        J12 = -alpha * n * v_ss**(n-1) / (1 + v_ss**n)**2  # âˆ‚(du/dt)/âˆ‚v\n",
    "        J21 = -alpha * n * u_ss**(n-1) / (1 + u_ss**n)**2  # âˆ‚(dv/dt)/âˆ‚u\n",
    "        J22 = -beta  # âˆ‚(dv/dt)/âˆ‚v\n",
    "        \n",
    "        J = np.array([[J11, J12], [J21, J22]])\n",
    "        \n",
    "        # Calculate eigenvalues\n",
    "        eigenvalues = np.linalg.eigvals(J)\n",
    "        \n",
    "        # Determine stability\n",
    "        if np.all(np.real(eigenvalues) < 0):\n",
    "            stability = \"Stable\"\n",
    "        elif np.all(np.real(eigenvalues) > 0):\n",
    "            stability = \"Unstable\"\n",
    "        else:\n",
    "            stability = \"Saddle\"\n",
    "        \n",
    "        return eigenvalues, stability\n",
    "    \n",
    "    # Find steady states\n",
    "    steady_states = find_steady_states()\n",
    "    \n",
    "    # Analyze each steady state\n",
    "    analysis_results = []\n",
    "    for i, ss in enumerate(steady_states):\n",
    "        eigenvals, stability = analyze_stability(ss)\n",
    "        analysis_results.append({\n",
    "            'steady_state_id': i,\n",
    "            'u': ss[0],\n",
    "            'v': ss[1],\n",
    "            'stability': stability,\n",
    "            'eigenvalue_1': eigenvals[0],\n",
    "            'eigenvalue_2': eigenvals[1]\n",
    "        })\n",
    "    \n",
    "    return pd.DataFrame(analysis_results)\n",
    "\n",
    "# Perform bistable switch analysis\n",
    "bistable_analysis = bistable_switch_analysis()\n",
    "\n",
    "print(\"Bistable Switch Steady-State Analysis:\")\n",
    "print(bistable_analysis.round(3))\n",
    "\n",
    "# Count stable states\n",
    "stable_states = bistable_analysis[bistable_analysis['stability'] == 'Stable']\n",
    "print(f\"\\nNumber of stable steady states: {len(stable_states)}\")\n",
    "print(\"This confirms bistability!\" if len(stable_states) == 2 else \"Not bistable.\")"
   ]
