   "source": [
    "# Simulate evolutionary trade-offs\n",
    "def evolutionary_tradeoff_analysis(n_individuals=1000):\n",
    "    \"\"\"\n",
    "    Simulate evolutionary trade-offs between growth rate and stress resistance\n",
    "    \"\"\"\n",
    "    # Generate random individuals with different trait values\n",
    "    growth_rate = np.random.uniform(0.1, 2.0, n_individuals)\n",
    "    \n",
    "    # Trade-off: higher growth rate reduces stress resistance\n",
    "    stress_resistance = 2.0 - 0.8 * growth_rate + np.random.normal(0, 0.2, n_individuals)\n",
    "    stress_resistance = np.maximum(stress_resistance, 0.1)  # Ensure positive\n",
    "    \n",
    "    # Calculate fitness under different conditions\n",
    "    # Normal conditions: growth rate is more important\n",
    "    fitness_normal = 0.8 * growth_rate + 0.2 * stress_resistance\n",
    "    \n",
    "    # Stress conditions: stress resistance is more important\n",
    "    fitness_stress = 0.2 * growth_rate + 0.8 * stress_resistance\n",
    "    \n",
    "    # Combined fitness (average across conditions)\n",
    "    fitness_combined = 0.5 * fitness_normal + 0.5 * fitness_stress\n",
    "    \n",
    "    return pd.DataFrame({\n",
    "        'growth_rate': growth_rate,\n",
    "        'stress_resistance': stress_resistance,\n",
    "        'fitness_normal': fitness_normal,\n",
    "        'fitness_stress': fitness_stress,\n",
    "        'fitness_combined': fitness_combined,\n",
    "        'individual': range(n_individuals)\n",
    "    })\n",
    "\n",
    "# Generate evolutionary data\n",
    "evo_df = evolutionary_tradeoff_analysis()\n",
    "\n",
    "# Find Pareto optimal individuals\n",
    "def find_pareto_optimal(df, obj1, obj2):\n",
    "    \"\"\"\n",
    "    Find Pareto optimal solutions for two objectives (both to be maximized)\n",
    "    \"\"\"\n",
    "    pareto_optimal = []\n",
    "    \n",
    "    for i, row in df.iterrows():\n",
    "        is_dominated = False\n",
    "        \n",
    "        for j, other_row in df.iterrows():\n",
    "            if i != j:\n",
    "                # Check if other individual dominates current individual\n",
    "                if (other_row[obj1] >= row[obj1] and other_row[obj2] >= row[obj2] and\n",
    "                    (other_row[obj1] > row[obj1] or other_row[obj2] > row[obj2])):\n",
    "                    is_dominated = True\n",
    "                    break\n",
    "        \n",
    "        if not is_dominated:\n",
    "            pareto_optimal.append(i)\n",
    "    \n",
    "    return pareto_optimal\n",
    "\n",
    "# Find Pareto optimal individuals\n",
    "pareto_indices = find_pareto_optimal(evo_df, 'growth_rate', 'stress_resistance')\n",
    "evo_df['is_pareto_optimal'] = False\n",
    "evo_df.loc[pareto_indices, 'is_pareto_optimal'] = True\n",
    "\n",
    "print(f\"Evolutionary analysis completed:\")\n",
    "print(f\"Total individuals: {len(evo_df)}\")\n",
    "print(f\"Pareto optimal individuals: {len(pareto_indices)}\")\n",
    "print(f\"Trade-off correlation: {evo_df['growth_rate'].corr(evo_df['stress_resistance']):.3f}\")"
   ]
